#include <stdio.h>
#include <string.h>

int main() {
    char s1[101], s2[101];
    
    // Read input strings
    scanf("%s", s1);
    scanf("%s", s2);
    
    // Compare strings and print in alphabetical order
    if (strcmp(s1, s2) < 0) {
        printf("%s%s", s1, s2);
    } else {
        printf("%s%s", s2, s1);
    }
    
    return 0;
}
