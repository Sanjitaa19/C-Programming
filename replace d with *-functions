#include <stdio.h>

void replaceDWithAsterisk(char* str) {
    for (int i = 0; str[i] != '\0'; i++) {
        if (str[i] == 'd') {
            str[i] = '*';
        }
    }
return str;
}

int main() {
    char str[] = "Hello world, how are you doing today?";
    
    replaceDWithAsterisk(str);
    
    printf("Modified string: %s\n", str);
    
    return 0;
}
