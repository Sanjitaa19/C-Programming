#include <stdio.h>
#include <stdlib.h>

int main() {
    int n, r1, r2, count = 0;
    scanf("%d", &n);
    int arr[n];

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    scanf("%d %d", &r1, &r2);

    for (int i = 0; i < n; i++) {
        if (arr[i] >= r1 && arr[i] <= r2) {
            count++;
        }
    }

    printf("%d\n", count);
    return 0;
}
