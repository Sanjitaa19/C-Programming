#include <stdio.h>
#include <string.h>

int main() {
    char stringvalue[100], charToFind;
    scanf("%s\n%c", stringvalue, &charToFind);

    int index = 0;

    while (stringvalue[index] != charToFind) {
        index++;
    }

    stringvalue[index] = '\0';
    printf("%s", stringvalue);

    return 0;
}
