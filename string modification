#include <stdio.h>

int main() {
    char s[1001];
    scanf("%s", s);
    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] != 'a' && s[i] != 'e' && s[i] != 'i' && s[i] != 'o' && s[i] != 'u') {
            printf(".%c", s[i]); // Print a full stop before the consonant
        }
    }
    return 0;
}
