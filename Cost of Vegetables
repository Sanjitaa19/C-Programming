#include<stdio.h>
#include<stdlib.h>

int main() {
    int n;
    scanf("%d",&n);

    int x[n]; 
    char y[n]; 

    for(int i=0;i<n;i++){
        scanf("%d %c",&x[i],&y[i]);
        if(y[i]=='C'){
            printf("%d\n",34*x[i]); 
        } else if(y[i]=='P'){
            printf("%d\n",20*x[i]); 
        } else if(y[i]=='T'){
            printf("%d\n",25*x[i]);
        } else if(y[i]=='S'){
            printf("%d\n",30*x[i]);
        }
    }
    return 0;
}
