#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[n];

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    for (int i = 0; i < n; i++) {
        if (arr[i] % 2 != 0) { // Find first odd number
            printf("%d ", arr[i]);
            break;
        }
    }

    for (int i = 0; i < n; i++) {
        if (arr[i] % 2 == 0) { // Find first even number
            printf("%d", arr[i]);
            break;
        }
    }

    return 0;
}
